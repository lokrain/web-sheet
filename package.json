{
  "name": "web-sheet",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.26.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "msw",
      "sharp",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "test": "NODE_ENV=test NODE_OPTIONS=--localstorage-file=.jest-localstorage jest",
    "test:watch": "NODE_ENV=test NODE_OPTIONS=--localstorage-file=.jest-localstorage jest --watch",
    "test:ci": "NODE_ENV=test NODE_OPTIONS=--localstorage-file=.jest-localstorage jest --ci",
    "test:cov": "NODE_ENV=test NODE_OPTIONS=--localstorage-file=.jest-localstorage jest --coverage",
    "bench": "node --expose-gc node_modules/.bin/tsx bench/bench-xml.ts",
    "bench:prof": "node --prof --expose-gc node_modules/.bin/tsx bench/bench-xml.ts",
    "perf:ci": "NODE_ENV=test NODE_OPTIONS=--localstorage-file=.jest-localstorage jest --ci --runInBand --coverage=false perf/perf.spec.ts",
    "perf:update-baseline": "node --expose-gc node_modules/.bin/tsx perf/update-baseline.ts",
    "skills:test": "bash scripts/test-skills.sh"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.8",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "shadcn": "^3.6.3",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=25 <26"
  }
}
